{
  "id": "flingGamePart1",
  "editorSwitches": {
    "OpenPropertiesManager": "Scene"
  },
  "flow": [
    {
      "id": "OpenPropertiesManager",
      "elementToHighlightId": "#main-toolbar-project-manager-button",
      "nextStepTrigger": {
        "presenceOfElement": "div[role=\"presentation\"]:not([aria-hidden=true]) #project-manager-tab-game-settings"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Open the **Project Manager**"
          }
        },
        "placement": "right"
      }
    },
    {
      "id": "",
      "elementToHighlightId": "#project-manager-tab-game-settings",
      "nextStepTrigger": {
        "presenceOfElement": "#project-manager-tab-game-properties"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Open the **Game Settings** tab."
          }
        }
      }
    },
    {
      "id": "",
      "elementToHighlightId": "#project-manager-tab-game-properties",
      "nextStepTrigger": {
        "presenceOfElement": "#project-properties-dialog"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Open the game properties."
          }
        }
      },
      "isOnClosableDialog": true
    },
    {
      "id": "",
      "elementToHighlightId": "#game-resolution-width",
      "nextStepTrigger": {
        "valueHasChanged": true
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Change the width to 1280 pixels."
          }
        }
      },
      "isOnClosableDialog": true
    },
    {
      "id": "",
      "elementToHighlightId": "#game-resolution-height",
      "nextStepTrigger": {
        "valueHasChanged": true
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Change the height to 720 pixels."
          }
        }
      },
      "isOnClosableDialog": true
    },
    {
      "id": "",
      "elementToHighlightId": "#game-resolution-resize-mode",
      "nextStepTrigger": {
        "valueHasChanged": true
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Set this to **no changes in game size**."
          }
        }
      },
      "isOnClosableDialog": true
    },
    {
      "id": "",
      "elementToHighlightId": "#project-properties-dialog #apply-button",
      "nextStepTrigger": {
        "absenceOfElement": "#project-properties-dialog"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "We're done."
          }
        }
      }
    },
    {
      "id": "ClickOnNewObjectButtonForBorder",
      "elementToHighlightId": "#add-new-object-button",
      "nextStepTrigger": {
        "presenceOfElement": "#new-object-dialog"
      },
      "tooltip": {
        "placement": "left",
        "description": {
          "messageByLocale": {
            "en": "First, let's find a border for our ring."
          }
        }
      }
    },
    {
      "elementToHighlightId": "#asset-store-tab",
      "nextStepTrigger": {
        "presenceOfElement": "#asset-store"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Let's choose an object from the asset store"
          }
        },
        "placement": "bottom"
      },
      "skippable": true,
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#asset-store-search-bar",
      "nextStepTrigger": {
        "valueHasChanged": true
      },
      "tooltip": {
        "title": {
          "messageByLocale": {
            "en": "We're looking for a tiled sprite that we can resize easily"
          }
        },
        "description": {
          "messageByLocale": {
            "en": "Tip: search for “tiled sprite”."
          }
        }
      },
      "skippable": true,
      "isOnClosableDialog": true
    },
    {
      "nextStepTrigger": {
        "presenceOfElement": "#add-asset-button"
      },
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#add-asset-button",
      "isTriggerFlickering": true,
      "nextStepTrigger": {
        "presenceOfElement": "#object-item-0"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Add this asset to your project."
          }
        }
      },
      "mapProjectData": {
        "border": "lastProjectObjectName"
      },
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#new-object-dialog #close-button",
      "nextStepTrigger": {
        "absenceOfElement": "#new-object-dialog"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Let's add the $(border) to our game!"
          }
        }
      }
    },
    {
      "elementToHighlightId": "#object-item-0",
      "nextStepTrigger": {
        "instanceAddedOnScene": "border"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Drag $(border) from the menu to the canvas."
          }
        },
        "placement": "left"
      }
    },
    {
      "nextStepTrigger": {
        "clickOnTooltipButton": "I'm done"
      },
      "tooltip": {
        "standalone": true,
        "description": {
          "messageByLocale": {
            "en": "Place and resize at least 4 instances of $(border) to create a closed frame around your screen ($(instancesCount:border)/4)."
          }
        },
        "placement": "top",
        "image": {
          "dataUrl": "data:image/svg+xml;base64,"
        }
      }
    },
    {
      "id": "ClickOnNewObjectButtonForProjectile",
      "elementToHighlightId": "#add-new-object-button",
      "nextStepTrigger": {
        "presenceOfElement": "#new-object-dialog"
      },
      "tooltip": {
        "placement": "left",
        "description": {
          "messageByLocale": {
            "en": "Now let's find an object that we can throw."
          }
        }
      }
    },
    {
      "elementToHighlightId": "#asset-store-tab",
      "nextStepTrigger": {
        "presenceOfElement": "#asset-store"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Let's choose an object from the asset store."
          }
        },
        "placement": "bottom"
      },
      "skippable": true,
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#asset-store-search-bar",
      "nextStepTrigger": {
        "valueHasChanged": true
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Select a circular sprite object that you'd like to throw."
          }
        }
      },
      "isOnClosableDialog": true,
      "shortcuts": [
        {
          "stepId": "CloseAssetStoreForCoin",
          "trigger": {
            "presenceOfElement": "#object-item-1"
          }
        }
      ]
    },
    {
      "nextStepTrigger": {
        "presenceOfElement": "#add-asset-button"
      },
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#add-asset-button",
      "isTriggerFlickering": true,
      "nextStepTrigger": {
        "presenceOfElement": "#object-item-1"
      },
      "mapProjectData": {
        "projectile": "lastProjectObjectName"
      },
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#new-object-dialog #close-button",
      "nextStepTrigger": {
        "absenceOfElement": "#new-object-dialog"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Perfect, let's see how to throw it easily."
          }
        }
      }
    },
    {
      "elementToHighlightId": "#object-item-1",
      "nextStepTrigger": {
        "instanceAddedOnScene": "projectile"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Add 3 instances of $(projectile) inside the frame."
          }
        }
      }
    },
    {
      "id": "ClickOnNewObjectButtonForTarget",
      "elementToHighlightId": "#add-new-object-button",
      "nextStepTrigger": {
        "presenceOfElement": "#new-object-dialog"
      },
      "tooltip": {
        "placement": "left",
        "description": {
          "messageByLocale": {
            "en": "Now let's find a target to aim."
          }
        }
      }
    },
    {
      "elementToHighlightId": "#asset-store-tab",
      "nextStepTrigger": {
        "presenceOfElement": "#asset-store"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Let's choose an object from the asset store."
          }
        },
        "placement": "bottom"
      },
      "skippable": true,
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#asset-store-search-bar",
      "nextStepTrigger": {
        "valueHasChanged": true
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Select an object that you would like to use as a target to throw $(projectile) at."
          }
        }
      },
      "isOnClosableDialog": true,
      "shortcuts": [
        {
          "stepId": "CloseAssetStoreForCoin",
          "trigger": {
            "presenceOfElement": "#object-item-2"
          }
        }
      ]
    },
    {
      "nextStepTrigger": {
        "presenceOfElement": "#add-asset-button"
      },
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#add-asset-button",
      "isTriggerFlickering": true,
      "nextStepTrigger": {
        "presenceOfElement": "#object-item-2"
      },
      "mapProjectData": {
        "target": "lastProjectObjectName"
      },
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#new-object-dialog #close-button",
      "nextStepTrigger": {
        "absenceOfElement": "#new-object-dialog"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Perfect, let's add it to the scene."
          }
        }
      }
    },
    {
      "elementToHighlightId": "#object-item-2",
      "nextStepTrigger": {
        "instanceAddedOnScene": "target"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Add $(target) inside the frame."
          }
        }
      }
    },
    {
      "id": "ClickOnNewObjectButtonForBlock",
      "elementToHighlightId": "#add-new-object-button",
      "nextStepTrigger": {
        "presenceOfElement": "#new-object-dialog"
      },
      "tooltip": {
        "placement": "left",
        "description": {
          "messageByLocale": {
            "en": "Finally let's find something to protect $(target)."
          }
        }
      }
    },
    {
      "elementToHighlightId": "#asset-store-tab",
      "nextStepTrigger": {
        "presenceOfElement": "#asset-store"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Let's choose an object from the asset store."
          }
        },
        "placement": "bottom"
      },
      "skippable": true,
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#asset-store-search-bar",
      "nextStepTrigger": {
        "valueHasChanged": true
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Select a block object you would like to use as an obstacle for your $(projectile)."
          }
        }
      },
      "isOnClosableDialog": true,
      "shortcuts": [
        {
          "stepId": "CloseAssetStoreForCoin",
          "trigger": {
            "presenceOfElement": "#object-item-3"
          }
        }
      ]
    },
    {
      "nextStepTrigger": {
        "presenceOfElement": "#add-asset-button"
      },
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#add-asset-button",
      "isTriggerFlickering": true,
      "nextStepTrigger": {
        "presenceOfElement": "#object-item-3"
      },
      "mapProjectData": {
        "block": "lastProjectObjectName"
      },
      "isOnClosableDialog": true
    },
    {
      "elementToHighlightId": "#new-object-dialog #close-button",
      "nextStepTrigger": {
        "absenceOfElement": "#new-object-dialog"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Perfect, let's add it to the scene."
          }
        }
      }
    },
    {
      "elementToHighlightId": "#object-item-3",
      "nextStepTrigger": {
        "instanceAddedOnScene": "block"
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": "Add multiple $(block)s inside the frame."
          }
        }
      }
    },
    {
      "id": "STOP",
      "elementToHighlightId": "",
      "nextStepTrigger": {
        "presenceOfElement": ""
      },
      "tooltip": {
        "description": {
          "messageByLocale": {
            "en": ""
          }
        }
      },
      "isOnClosableDialog": true
    }
  ]
}